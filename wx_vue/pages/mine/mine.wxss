.heads{
  position: fixed;
  top: 0px;
  height: auto;
  width: 100%;
  display: flex;
  flex-direction: column;
  box-shadow:0px 0px 5px rgb(189, 186, 186);
  background-color: #1989fa;
  margin-bottom: 14px;
}

.head{
  font-size: 14px;
  background-color: #1989fa;
  width: 100%;
  height: 100px;
  display: flex;
  flex-direction: row;
}
.head_img{
  margin: 20px 10px 20px 20px;
  height: 60px;
  width: 60px;
  border-radius: 50%;
  border-style:solid;
  border-width:2px;
  border-color:#dbdee4;
}
.head_view{
  display: flex;
  flex-direction: column;
  height: 100%;
  width: auto;
  color: #fff;
}
.head_view_name{
  font-size: 15px;
  height: 48px;
  display: flex;
  flex-direction: column-reverse;
}
.head_view_info{
  margin-top: 4px;
  font-size: 12px;
  height: 48px;
  display: flex;
  flex-direction: column;
  white-space:nowrap;
}
.head_jt{
  right: 20px;
  position:absolute;
  line-height: 100px;
}

van-cell{
  font-size: 13px;
}

.line{
  background-color: #eeeff0;
  height: 1px;
  width: 100%;
}
.list{
  padding-left: 20px;
  padding-right: 20px;
  margin-top: 50px;
}




.tlist{
  margin-bottom: 10px;
  height: 40px;
  width: 100%;
  color: #fff;
  display: flex;
  flex-direction: row;
}
.tlist_v{
  font-size: 13px;
  width: 26.66%;
  margin-left: 3%;
  margin-right: 3%;
  display: flex;
  flex-direction: column;
  align-content: center;
  text-align: center;
  justify-content:center;
  align-items:Center;
}
.l_2{
  height: 24px;
  margin-top: 7px;
  width: 1px;
  background-color: #dbdee4;
}



.body{
  height: auto;
  width: 100%;
  display: flex;
  flex-direction: column;
}