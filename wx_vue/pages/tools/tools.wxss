.list{
  height: auto;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
}
.list_box{
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 10px;
  margin-left: 2.4%;
  width: 22%;
  height: 100%;
}

.list_box_box{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.list_box_src{
  height: 65%;
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: center;
  align-items: center;
}
.list_box_img{
  margin: 0 auto;
  height: 43px;
  width: 43px;
}
.list_box_text{
  
  height: 35%;
  width: 100%;
  text-align: center;
  font-size: 13px;
  color: #fff;
}





